                "filters": {
                  "kind": "association"
                }
              },
              {
                "id": "municipalities",
                "display": "רשויות מקומיות",
                "filters": {
                  "kind": "municipality"
                }
              },
              {
                "id": "other",
                "display": "סוגים אחרים",
                "filters": {
                  "kind__not": ["company", "association", "municipality"]
                }
              }
            ]
          }
        ]
      },
      {
        "id": "procurement",
        "name": "הליכי רכש",
